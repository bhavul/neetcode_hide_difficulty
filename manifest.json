{
    "manifest_version": 3,
    "name": "Hide Neetcode Difficulty",
    "version": "1.0",
    "description": "This extension hides the difficulty level when on neetcode.io website.",
    "icons": {
        "16": "icons-16.png",
        "32": "icons-32.png",
        "96": "icons-96.png"
    },
    "author": {
        "name": "Bhavul Gauri"
    },
    "host_permissions": ["https://neetcode.io/practice"],
    "content_scripts": [
      {
        "matches": ["https://neetcode.io/practice"],
        "js": ["content.js"]
      }
    ],
    "permissions": [
      "activeTab"
    ]
  }